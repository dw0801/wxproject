<Tabs tabs="{{tabs}}" bindtabsItemChange="handldTabsItemChange">
  <view class="order_main">
    <view class="order_item" wx:for="{{orders}}" wx:key="{{order_id}}">
      <view class="order_no_row">
        <view class="order_no_text">订单编号</view>
        <view class="order_no_vaule">{{item.order_number}}</view>
      </view>
      <view class="order_price_row">
        <view class="order_price_text">订单价格</view>
        <view class="order_price_vaule">￥{{item.total_price}}</view>
      </view>
      <view class="order_time_row">
        <view class="order_time_text">订单时间</view>
        <view class="order_time_vaule">{{item.create_time_cn}}</view>
      </view>
    </view>
  </view>
</Tabs>